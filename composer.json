{
    "name": "reecem/ignition-git",
    "description": "Open a Github, Gitlab Issue from Error page ",
    "keywords": [
        "laravel",
        "flare",
        "ignition"
    ],
    "homepage": "https://github.com/ReeceM/ignition-git",
    "license": "MIT",
    "authors": [
        {
            "name": "Reece May",
            "email": "ziggyelec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "facade/ignition": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ReeceM\\GitTab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReeceM\\GitTab\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReeceM\\GitTab\\TabServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
